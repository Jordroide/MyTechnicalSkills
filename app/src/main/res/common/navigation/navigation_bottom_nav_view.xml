<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_navigation"
    app:startDestination="@+id/navigation_fragment_quote">

    <fragment
        android:id="@+id/navigation_fragment_quote"
        android:name="com.jordroid.showcase.quotes.presenter.view.QuoteRandomFragment"
        android:label="@string/fragment_quote_title"
        tools:layout="@layout/quote_fragment" />

    <fragment
        android:id="@+id/navigation_fragment_list"
        android:name="com.jordroid.showcase.browsing.presenter.view.BrowsingAnimeFragment"
        android:label="@string/fragment_list_title"
        tools:layout="@layout/browsing_anime_fragment" />

    <fragment
        android:id="@+id/navigation_fragment_gallery"
        android:name="com.jordroid.showcase.gallery.presenter.view.GalleryFragment"
        android:label="@string/fragment_gallery_title"
        tools:layout="@layout/gallery_fragment" />

    <fragment
        android:id="@+id/navigation_fragment_cv"
        android:name="com.jordroid.showcase.cv.presenter.view.FragmentCv"
        android:label="@string/fragment_cv_title"
        tools:layout="@layout/cv_fragment" />

</navigation>
